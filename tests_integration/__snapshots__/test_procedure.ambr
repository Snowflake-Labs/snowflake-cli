      handler: "app.hello_function"
      signature:
        - name: "name"
          type: "string"
      returns: string
  procedures:
    - name: hello
      handler: "app.hello_procedure"
      signature:
        - name: "name"
          type: "string"
      returns: string
    - name: test
      handler: "app.test_procedure"
      signature: ""
      returns: string
  
